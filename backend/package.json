    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "serverless": "^2.9.0",
    "serverless-offline": "^6.8.0",
    "typescript": "^4.6.3"
  }
}
