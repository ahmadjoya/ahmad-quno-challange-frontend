{
  "name": "quno-challenge-backend",
  "version": "1.0.0",
  "description": "Backend to support the quno challenge",
  "main": "index.js",
  "author": "Qunomedical",
  "license": "MIT",
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "build": "tsc --build --verbose",
