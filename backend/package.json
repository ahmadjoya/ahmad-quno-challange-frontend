    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "serverless": "^3.25.1",
    "serverless-offline": "^12.0.2",
    "typescript": "^4.9.4"
  }
}
